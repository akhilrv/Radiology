<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.openmrs.module.radiology">
	<class name="Study" table="radiology_study">
		<id name="id" type="int" column="id" unsaved-value="0">
			<generator class="native" />
		</id>
		<property name="uid" column="uid"></property>
		<many-to-one name="order" class="org.openmrs.Order"
			not-null="true">
			<column name="order_id"></column>
		</many-to-one>
		<property name="scheduledStatus" column="scheduled_status">
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">org.openmrs.module.radiology.ScheduledProcedureStepStatus</param>
			</type>
		</property>
		<property name="performedStatus" column="performed_status">
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">org.openmrs.module.radiology.PerformedProcedureStepStatus</param>
			</type>
		</property>
		<property name="priority" column="priority" not-null="true">
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">org.openmrs.module.radiology.RequestedProcedurePriority</param>
			</type>
		</property>
		<property name="modality" column="modality" not-null="true">
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">org.openmrs.module.radiology.Modality</param>
				<param name="type">12</param>
				<param name="useNamed">true</param>
			</type>
		</property>
		<property name="mwlStatus" column="mwl_status" not-null="true">
			<type name="org.hibernate.type.EnumType">
				<param name="enumClass">org.openmrs.module.radiology.MwlStatus</param>
			</type>
		</property>
		<many-to-one name="scheduler" class="org.openmrs.User"
			not-null="false" not-found="ignore">
			<column name="scheduler_user_id"></column>
		</many-to-one>
		<many-to-one name="performingPhysician" class="org.openmrs.User"
			not-null="false" not-found="ignore">
			<column name="performing_physician_user_id"></column>
		</many-to-one>
		<many-to-one name="readingPhysician" class="org.openmrs.User"
			not-null="false" not-found="ignore">
			<column name="reading_physician_user_id"></column>
		</many-to-one>
	</class>
</hibernate-mapping>